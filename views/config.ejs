<!-- Modal -->
<div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <h3 id="myModalLabel">Configure device</h3>
  </div>
  <div class="modal-body">
    <form class="form-inline">
      <%for (var i=0;i<forms.length;i++) {%>
          <label class="" for="<%=forms[i]%>" > <%=forms[i]%></label>
          <input class="input-small" type="text" id="<%=forms[i]%>" placeholder="<%=forms[i]%>">
        <% } %>
        <table class="table table-hover table-condensed">
          <thead>
             <tr>
                 <td>OID</td>
                 <td>Error</td>
                 <td>Error Message</td>
                 <td>Warn</td>
                 <td>Warn Message</td>
                 <td>Clear</td>
                 <td>Clear Message</td>
                 <td>State</td>
                 <td></td>
             </tr>
         </thead>
         <tbody id="oids">
         </tbody>
      </table>
   </form>
  <button class="btn" onclick="addOidRow()">Add OID</button>
  </div>
  <div class="modal-footer">
    <button class="btn btn-danger" data-dismiss="modal" aria-hidden="true" style="float:left" onclick="deleteHost()">Delete device</button>
    <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
    <button class="btn btn-primary" onclick="postHost()">Save changes</button>
  </div>
</div>